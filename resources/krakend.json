{
  "version": 2,
  "name": "Test API Gateway",
  "port": 8000,
  "cache_ttl": "3600s",
  "timeout": "3s",
  "extra_config": {},
  "output_encoding": "json",
  "endpoints": [
    {
      "endpoint": "/findNearbyRestaurants",
      "extra_config": {
        "github.com/devopsfaith/krakend/proxy": {
          "sequential": true
        }
      },
      "method": "POST",
      "backend": [
        {
          "is_collection": true,
          "url_pattern": "/api/v1/geo",
          "host": [
            "http://localhost:3001"
          ]
        }
      ]
    }
  ]
}
